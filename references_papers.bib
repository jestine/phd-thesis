volume = {24},
year = {2008},
month = {Jan},
language = {English},
keywords = {Stress, Algorithm, Explicit Integration, Damage, Solids, Single, Polycrystals, Integration Algorithms, Flow, Crystal Plasticity, Efficient, Dynamic Deformation}, 
date-added = {2010-12-08 20:43:13 +0000},
date-modified = {2010-12-09 12:02:43 +0000},
