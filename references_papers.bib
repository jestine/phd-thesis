@ProceedingsPaper{Ishijima:2002p55,
author = {Y Ishijima and K Kakiuchi and T Furuya and H Kurishita and M Hasegawa and T Igarashi and M Kawai}, 
journal = {J Nucl Mater},
title = {Corrosion resistance of refractory metals in high-temperature water},
abstract = {The high-temperature water corrosion resistance of refractory transition metals of group IVa, Va and VIa was studied Corrosion tests were conducted for 17 kinds of tungsten, 12 kinds of molybdenum and other 9 kinds of metals in flowing water with a dissolved oxygen content of 400 ppb and pH 6-7 at 180, 260 and 320 degreesC for 200 h. it is shown that the corrosion rate of W and Mo depends strongly on temperature and material. W and Mo are not corroded severely at 180 degreesC, the corrosion rates are 0.001-0.047 mm/y for W and 0.022-0.029 mm/y for Mo. The heavy-alloys are corroded severely at 260 degreesC, the corrosion rates are 0.141-0.214 mm/y. At 320 degreesC, significant corrosion attack occurs depending on the materials. CrN coated W shows excellent corrosion resistance, the rate,is as low as similar or equal to0.004 mm/y. W-Re and Mo-Re alloys are corroded severely, with a maximum rate of 1.656 mm/y for W-2\%Re and 0.405 mm/y for Mo-17.7\%Re. Mo shows a considerably higher corrosion resistance than W. All group lVa and Va metals except Nb, as well as Cr, Re and SUS316 at 320 degreesC show better resistance than Mo. (C) 2002 Elsevier Science B.V. All rights reserved.},
